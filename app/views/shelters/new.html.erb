<form action="/shelters" method="post">
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
  <h3>Add a new shelter:</h3>
  <label for='name'>Shelter Name:</label>
  <input type= 'text' id='name' name'shelter[name]'/><br/>
  <label for='address'>Street Address:</label>
  <input type= 'text' id='address' name'shelter[address]'/><br/>
  <label for='city'>City:</label>
  <input type= 'text' id='city' name'shelter[city]'/><br/>
  <label for='state'>State:</label>
  <input type= 'text' id='state' name'shelter[state]'/><br/>
  <label for='zip'>Zip Code:</label>
  <input type= 'text' id='zip' name'shelter[zip]'/><br/>
  <input type='submit' value="Submit"/>
</form>
